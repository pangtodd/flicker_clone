<%= render "layouts/errors", :object => @image %>

<%= form_for @image do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :comment%>
  <%= f.text_field :comment %>
  <%= f.label :tag%>
  <%= f.text_field :tag %>
  <%# dropdown %>
  <%= f.label :image %>
  <%= f.file_field :image, multiple: true%>
  <%= f.submit %>
<% end %>